<?xml version="1.0" ?>
<odoo>
    <data>

        <template id="assets_frontend_mypage" inherit_id="website.assets_frontend" name="Mypage Frontend Assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/website_vote/static/src/scss/vote.scss" t-ignore="true"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/website_vote/static/src/js/toggle_widget.js"></script>
            </xpath>
        </template>

    </data>
</odoo>
